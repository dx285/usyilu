                            people+='$'+row.adultprice+'/双人间/人 ';
                        }
                        if(row.childprice>0){
                            people+='$'+row.childprice+'/三人间/人 ';
                        }
                        if(row.oldprice>0){
                            people+='$'+row.oldprice+'/单人间/人 ';
                        }
                        html+='<option value="'+row.useday+'">'+row.shortdate+'('+row.weekday+')'+people+'</option>';
