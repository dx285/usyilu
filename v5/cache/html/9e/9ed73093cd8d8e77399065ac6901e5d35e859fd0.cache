                }
                else{
                    html+='<option value="0">请选择日期</option>';
                    $(".btn-yd").text('不可预订');
                    $(".btn-yd").attr('style','background:#ccc');
                    $(".btn-yd").removeClass('gobook');
                }
                $('.date-list').append(html);
            }
        })
    }

});
