    //预订页面
    $('body').delegate('.gobook','click',function(){
        var usedate = $('.date-list').val();
        var suitid = $('#suitid').val();
        var lineid = $('#lineid').val();
        if(usedate == null){
            return false;
        }
        var url = "http://www.usyilu.com/lines/book/?usedate="+usedate+'&suitid='+suitid+"&lineid="+lineid;
