            $('.jifentprice').hide();
        }
        if(jifenbook){
            $('.jifenbook').html(jifenbook+'分<i></i>')
        }else{
            $('.jifenbook').hide();
        }
        $("#suitid").val(suitid);
        $(this).addClass('on').siblings().removeClass('on');
        var lineid = $("#lineid").val();
        get_calendar(suitid,lineid);
        get_date_list(suitid,lineid);
