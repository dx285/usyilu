    //获取日历下拉列表
    function get_date_list(suitid,lineid){
        $.ajax({
            type:'POST',
            url:SITEURL+'line/ajax_date_options',
            data:{suitid:suitid,lineid:lineid},
            dataType:'json',
            success:function(data){
                $('.date-list').empty();
